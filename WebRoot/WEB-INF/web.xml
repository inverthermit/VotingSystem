<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DataServlet</servlet-name>
    <servlet-class>servlet.DataServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>NewVoteServlet</servlet-name>
    <servlet-class>servlet.NewVoteServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GenExcelServlet</servlet-name>
    <servlet-class>servlet.GenExcelServlet</servlet-class>
  </servlet>


<welcome-file-list>
<welcome-file>index.html</welcome-file>
</welcome-file-list>
  <servlet-mapping>
    <servlet-name>DataServlet</servlet-name>
    <url-pattern>/servlet/DataServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewVoteServlet</servlet-name>
    <url-pattern>/servlet/NewVoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GenExcelServlet</servlet-name>
    <url-pattern>/servlet/GenExcelServlet</url-pattern>
  </servlet-mapping>

</web-app>